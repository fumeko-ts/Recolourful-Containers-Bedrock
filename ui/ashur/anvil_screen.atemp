{
  "namespace": "anvil_screen",
  "anvil_panel@container_common.container_details": {
    "size": [ 176, 166 ],
    "$screen_controls": [
      {"common_panel@screen_content.container_base": {"$anvil_screen": true}},
      {
        "anvil_top_half@global.empty_stack": {
          "orientation": "vertical",
          "size": [ 176, 66 ],
          "anchor_to": "top_middle",
          "anchor_from": "top_middle",
          "controls": [
            {"filler_1@global.empty_panel": {"size": [ "fill", 6 ]}},
            {
              "anvil_label_dynamic?@global.empty_stack": {
                "orientation": "horizontal",
                "size": [ 176, 14 ],
                "controls": [
                  {"filler_1@global.empty_panel": {"size": [ 60, "fill" ]}},
                  {"chest_label_holder@global.empty_panel": {"size": [ 116, 14 ]}}
                ]
              }
            },
            {
              "anvil_text_edit@global.empty_stack": {
                "orientation": "horizontal",
                "size": [ 176, 16 ],
                "controls": [
                  {"filler_1@global.empty_panel": {"size": [ 59, "fill" ]}},
                  {"text_edit@anvil_screen.text_edit_panel": {}},
                  {"filler_2@global.empty_panel": {"size": [ 7, "fill" ]}}
                ]
              }
            },
            {"filler_2@global.empty_panel": {"size": [ "fill", 9 ]}},
            {
              "anvil_cells@global.empty_stack": {
                "orientation": "horizontal",
                "size": [ 176, 21 ],
                "controls": [
                  {"filler_1@global.empty_panel": {"size": [ 26, "fill" ]}},
                  {
                    "stack_1@anvil_screen.cell_vertical": {
                      "controls": [
                        {"filler_1@global.empty_panel": {"size": [ "fill", 1 ]}},
                        {"input_item_slot@global.container_item": {"$item_collection": "anvil_input_items"}},
                        {"filler_2@global.empty_panel": {"size": [ "fill", 2 ]}}
                      ]
                    }
                  },
                  {"filler_2@global.empty_panel": {"size": [ 31, "fill" ]}},
                  {
                    "stack_2@anvil_screen.cell_vertical": {
                      "controls": [
                        {"filler_1@global.empty_panel": {"size": [ "fill", 1 ]}},
                        {"material_item_slot@global.container_item": {"$item_collection": "anvil_material_items"}},
                        {"filler_2@global.empty_panel": {"size": [ "fill", 2 ]}}
                      ]
                    }
                  },
                  {"filler_3@global.empty_panel": {"size": [ 6, "fill" ]}},
                  {
                    "anvil_ex_holder@global.empty_panel": {
                      "size": [ 28, 21 ],
                      "controls": [
                        {
                          "cross_out_icon@screen_content.image_element": {
                            "texture": "",
                            "allow_debug_missing_texture": false,
                            "size": [ 28, 21 ],
                            "keep_ratio": true,
                            "bindings": [ {"binding_name": "(not #cross_out_icon)", "binding_name_override": "#visible"} ]
                          }
                        },
                        {
                          "cross_out_icon@screen_content.image_element": {
                            "texture": "assets/minecraft/textures/gui/sprites/container/anvil/error.png",
                            "size": [ 28, 21 ],
                            "keep_ratio": true,
                            "bindings": [ {"binding_name": "#cross_out_icon", "binding_name_override": "#visible"} ]
                          }
                        }
                      ]
                    }
                  },
                  {"filler_4@global.empty_panel": {"size": [ 6, "fill" ]}},
                  {
                    "stack_3@anvil_screen.cell_vertical": {
                      "controls": [
                        {"filler_1@global.empty_panel": {"size": [ "fill", 1 ]}},
                        {"result_item_slot@global.container_item": {"$item_collection": "anvil_result_items"}},
                        {"filler_2@global.empty_panel": {"size": [ "fill", 2 ]}}
                      ]
                    }
                  },
                  {"filler_5@global.empty_panel": {"size": [ 25, "fill" ]}}
                ]
              }
            }
          ]
        }
      },
      {
        "cost@global.empty_panel": {
          "size": [ 169, 79 ],
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "controls": [
            {
              "cost_labels@global.empty_panel": {
                "size": [ 104, 8 ],
                "anchor_to": "bottom_right",
                "anchor_from": "bottom_right",
                "controls": [
                  {
                    "bg1": {
                      "type": "image",
                      "texture": "textures/ui/Gray",
                      "layer": 4,
                      "size": [ 107, 13 ],
                      "offset": [ -2, 0 ],
                      "alpha": 0.3,
                      "bindings": [ {"binding_name": "#cost_text_red", "binding_name_override": "#visible"} ]
                    }
                  },
                  {
                    "bg2": {
                      "type": "image",
                      "texture": "textures/ui/Gray",
                      "layer": 4,
                      "size": [ 107, 13 ],
                      "offset": [ -2, 0 ],
                      "alpha": 0.3,
                      "bindings": [ {"binding_name": "#cost_text_green", "binding_name_override": "#visible"} ]
                    }
                  },
                  {
                    "cost_label_0@anvil_screen.cost_label": {"color": "$anvil_cost_fail_text_color", "$cost_bindings": "#cost_text_red"}
                  },
                  {
                    "cost_label_1@anvil_screen.cost_label": {"color": [ 0.494, 0.988, 0.125, 1.0 ], "$cost_bindings": "#cost_text_green"}
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "bottom@crafting_screen.inventory_bottom_half_with_label": {"anchor_to": "bottom_middle", "anchor_from": "bottom_middle"}
      }
    ]
  },
  "text_edit_panel": {
    "type": "panel",
    "size": [ 110, 16 ],
    "controls": [
      {
        "anvil_text_box@text_edit_box.text_field": {
          "offset": [ -1, -1 ],
          "$text_panel_size_minus_1px|default": [ 109, 16 ],
          "$text_edit_box_label_size": [ "default", 10 ],
          "property_bag": {"#property_field": "#item_name"},
          "$shadow": true,
          "max_length": 30,
          "$text_clear_button_enabled": false,
          "$text_box_bg_offset": [ 1, 1 ],
          "layer": 6,
          "$text_clear_offset": [ 14, 1 ],
          "$text_edit_box_label_offset": [ 4, 2 ],
          "$text_background_locked": "screen_content.anvil_text_field",
          "$text_background_default": "screen_content.anvil_text_field",
          "$text_background_hover": "screen_content.anvil_text_field_highlighted",
          "$text_edit_binding_name": "#text_box_item_name"
        }
      }
    ]
  },
  "cell_vertical@global.empty_stack": {"$stacking": "vertical", "orientation": "$stacking", "size": [ 18, 21 ]},
  "cost_label": {
    "type": "label",
    "shadow": true,
    "layer": 10,
    "text": "#text",
    "bindings": [
      {"binding_name": "#cost_text", "binding_name_override": "#text"},
      {"binding_name": "$cost_bindings", "binding_name_override": "#visible"}
    ]
  }
}