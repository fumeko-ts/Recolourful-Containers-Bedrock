    }
  ],
  "$recipe_book_contents": [
    { "offset_content@$page_contents": { "offset": [0.9, -1.6], "layer": 4 } },
    {
      "holder@global.empty_stack": {
        "orientation": "vertical",
        "ignored": "($is_in_recipe_inventory = false)",
        "anchor_to": "top_middle",
        "anchor_from": "top_middle",
        "size": [111, 30],
        "offset": [8, 0],
        "controls": [
          { "buffer_1@global.empty_panel": { "size": [111, 11] } },
          {
            "search_and_filter@global.empty_stack": {
              "orientation": "horizontal",
              "size": [111, 18],
              "controls": [
                {
                  "text_edit_panel@global.empty_panel": {
                    "size": [81, 18],
                    "controls": [
                      {
                        "text_edit_control@text_edit_box.text_edit_box": {
                          "size": [81, 14],
                          "property_bag": { "#property_field": "#item_name" },
                          "max_length": 300000,
                          "anchor_from": "center",
                          "anchor_to": "center",
                          "$text_clear_button_offset": [3, 0],
                          "$text_box_text_color": "white",
                          "$text_edit_box_label_offset": [1, 0.4],
                          "$text_clear_button_enabled": true,
                          "$place_holder_text": "§o§8§7Search...",

                          "$text_background_locked": "screen_content.text_field",
                          "$text_background_default": "screen_content.text_field",
                          "$text_background_hover": "screen_content.text_field_highlighted",

                          "$text_edit_binding_name": "#text_box_item_name",
                          "$text_edit_box_label_size": ["default", 10]
                        }
                      }
                    ]
                  }
                },
                { "buffer_2@global.empty_panel": { "size": [4, "fill"] } },
                { "filter@crafting_common.filter_toggle": {} }
              ]
            }
          }
        ]
      }
    },
    {
      "stack_holder@global.empty_stack": {
        "orientation": "vertical",
        "anchor_to": "bottom_middle",
        "anchor_from": "bottom_middle",
        "size": [77, 30],
        "controls": [
          {
            "stack@global.empty_stack": {
              "orientation": "horizontal",
              "size": [77, 17],
              "anchor_to": "bottom_middle",
              "anchor_from": "bottom_middle",
              "layer": 20,
              "controls": [
                { "buffer@global.empty_panel": { "size": [18, 0], "ignored": "(not ($current_page = 1))" } },
                { "left@pagination.tepes_left_button": {} },
                {
                  "buffer1@global.empty_panel": {
                    "size": [43, 17],
                    "controls": [
                      { "label@pagination.page_count_label": {} },
                      { "max_page_label@pagination.max_page_label": {} }
                    ]
                  }
                },
                { "right@pagination.tepes_right_button": {} }
              ]
            }
          },
          { "buffer_fill_1@global.empty_panel": { "size": [65, 13] } }
        ]
      }
    }
