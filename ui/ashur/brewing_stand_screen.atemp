{
  "namespace": "brewing_stand_screen",
  "brewing_panel@container_common.container_details": {
    "size": [176, 166],
    "$screen_controls": [
        {
        "common_panel@screen_content.container_base": { "$brewing_stand_screen": true }
        },
        {
        "sprite_stack@global.empty_stack": {
          "size": [176, 48],
          "orientation": "horizontal",
          "anchor_to": "top_middle",
          "anchor_from": "top_middle",
          "controls": [
              {
              "filler_1@global.empty_panel": { "size": [60, "fill"] }
              },
              {
              "bubbles_and_fuel@global.empty_stack": {
                "orientation": "vertical",
                "size": [18, 48],
                "controls": [
                    {
                    "filler_1@global.empty_panel": { "size": ["fill", 14] }
                    },
                    {
                    "bubbles_holder@global.empty_stack": {
                      "orientation": "horizontal",
                      "size": [18, 29],
                      "controls": [
                          {
                          "filler_1@global.empty_panel": { "size": [3, "fill"] }
                          },
                          {
                          "bubbles@brewing_stand_screen.image_panel": {
                            "$sprite": "bubbles.png",
                            "$image_size": [12, 29],
                            "$houkou": "down",
                            "$type": "bubbles"
                            }
                          },
                          {
                          "filler_2@global.empty_panel": { "size": [3, "fill"] }
                          }
                        ]
                      }
                    },
                    {
                    "filler_2@global.empty_panel": { "size": ["fill", 1] }
                    },
                    {
                    "brewing_fuel_bar@brewing_stand_screen.image_panel": {
                      "$sprite": "fuel_length.png",
                      "$image_size": [18, 4],
                      "$houkou": "left",
                      "$type": "fuel"
                      }
                    }
                  ]
                }
              },
              {
              "filler_2@global.empty_panel": { "size": [19, "fill"] }
              },
              {
              "arrow_stack@global.empty_stack": {
                "orientation": "vertical",
                "size": [9, 48],
                "controls": [
                    {
                    "filler_1@global.empty_panel": { "size": ["fill", 16] }
                    },
                    {
                    "brewing_arrow@brewing_stand_screen.image_panel": {
                      "$sprite": "brew_progress.png",
                      "$image_size": [9, 28],
                      "$houkou": "up",
                      "$type": "arrow"
                      }
                    },
                    {
                    "filler_2@global.empty_panel": { "size": ["fill", 4] }
                    }
                  ]
                }
              },
              {
              "filler_3@global.empty_panel": { "size": [70, "fill"] }
              }
            ]
          }
        },
        {
        "brewing_top_stack@global.empty_stack": {
          "orientation": "vertical",
          "size": [176, 75],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "controls": [
              {
              "filler_1@global.empty_panel": { "size": ["fill", 6] }
              },
              {
              "label@global.empty_stack": {
                "orientation": "horizontal",
                "size": [176, 10],
                "controls": [
                    {
                    "filler_1@global.empty_panel": { "size": [53, "fill"] }
                    },
                    {
                    "brewing_label@global.empty_panel": { "size": [123, "fill"] }
                    }
                  ]
                }
              },
              {
              "main_content@global.empty_stack": {
                "orientation": "horizontal",
                "size": [176, 59],
                "controls": [
                    {
                    "filler_1@global.empty_panel": { "size": [16, "fill"] }
                    },
                    {
                    "blaze_powder@global.empty_stack": {
                      "orientation": "vertical",
                      "size": [18, 59],
                      "controls": [
                          {
                          "fuel_item@global.container_item": { "$item_collection": "brewing_fuel_item" }
                          },
                          {
                          "filler_1@global.empty_panel": { "size": ["fill", 41] }
                          }
                        ]
                      }
                    },
                    {
                    "filler_2@global.empty_panel": { "size": [21, "fill"] }
                    },
                    {
                    "bottle_output_1@brewing_stand_screen.collection_template": {
                      "controls": [
                          {
                          "filler_1@global.empty_panel": { "size": ["fill", 34] }
                          },
                          {
                          "bottle_left@global.container_item": {
                            "$item_collection": "$collection_type",
                            "collection_index": 0
                            }
                          },
                          {
                          "filler_2@global.empty_panel": { "size": ["fill", 7] }
                          }
                        ]
                      }
                    },
                    {
                    "filler_3@global.empty_panel": { "size": [5, "fill"] }
                    },
                    {
                    "bottle_output_2@brewing_stand_screen.collection_template": {
                      "controls": [
                          {
                          "input_item@global.container_item": {
                            "$item_collection": "brewing_input_item",
                            "$collection_type": "brewing_input_item"
                            }
                          },
                          {
                          "filler_1@global.empty_panel": { "size": ["fill", 23] }
                          },
                          {
                          "bottle_middle@global.container_item": {
                            "$item_collection": "$collection_type",
                            "collection_index": 1
                            }
                          }
                        ]
                      }
                    },
                    {
                    "filler_4@global.empty_panel": { "size": [5, "fill"] }
                    },
                    {
                    "bottle_output_3@brewing_stand_screen.collection_template": {
                      "controls": [
                          {
                          "filler_1@global.empty_panel": { "size": ["fill", 34] }
                          },
                          {
                          "bottom_right@global.container_item": {
                            "$item_collection": "$collection_type",
                            "collection_index": 2
                            }
                          },
                          {
                          "filler_2@global.empty_panel": { "size": ["fill", 7] }
                          }
                        ]
                      }
                    },
                    {
                    "filler_5@global.empty_panel": { "size": [57, "fill"] }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
        "bottom@crafting_screen.inventory_bottom_half_with_label": {
          "anchor_to": "bottom_middle",
          "anchor_from": "bottom_middle"
          }
        }
      ]
    },
  "collection_template@global.empty_stack": {
    "orientation": "vertical",
    "size": [18, 59],
    "$collection_type": "brewing_result_items",
    "collection_name": "$collection_type"
    },
  "image_panel@global.empty_panel": {
    "size": "$image_size",
    "$sprite_src|default": "('assets/minecraft/textures/gui/sprites/container/brewing_stand/' + $sprite)",
    "controls": [
        {
        "brewing_texture_template@screen_content.image_element": {
          "layer": 3,
          "clip_direction": "$houkou",
          "size": "$image_size",
          "texture": "$sprite_src",
          "clip_pixelperfect": true,
          "bindings": [
              {
              "binding_name": "('#brewing_' + $type + '_ratio')",
              "binding_name_override": "#clip_ratio",
              "binding_condition": "always"
              }
            ]
          }
        }
      ]
    }
  }