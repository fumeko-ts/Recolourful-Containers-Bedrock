{
  "namespace": "shulker_screen",
  "shulker_box@container_common.container_details": {
    "type": "panel",
    "size": [176, 166],
    "$screen_controls": [
        {
        "common_panel@screen_content.container_base": { "$shulker_screen": true }
        },
        {
        "shulker_top_half@global.empty_stack": {
          "orientation": "vertical",
          "size": [176, 77],
          "anchor_to": "top_middle",
          "anchor_from": "top_middle",
          "controls": [
              {
              "label_holder@global.empty_stack": {
                "orientation": "horizontal",
                "size": [176, 17],
                "controls": [
                    {
                    "filler_1@global.empty_panel": { "size": [8, "fill"] }
                    },
                    {
                    "chest_label_holder@global.empty_panel": { "size": [57, 7] }
                    },
                    {
                    "filler_2@global.empty_panel": { "size": [111, "fill"] }
                    }
                  ]
                }
              },
              {
              "chest_grid_holder@global.empty_stack": {
                "orientation": "horizontal",
                "size": [176, 54],
                "controls": [
                    {
                    "filler_3@global.empty_panel": { "size": [7, "fill"] }
                    },
                    {
                    "shulker_grid": {
                      "type": "grid",
                      "size": [162, 54],
                      "grid_dimensions": [9, 3],
                      "grid_item_template": "common.container_item",
                      "$item_collection_name": "container_items",
                      "collection_name": "container_items"
                      }
                    },
                    {
                    "filler_4@global.empty_panel": { "size": [7, "fill"] }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
        "bottom@crafting_screen.inventory_bottom_half_with_label": {
          "anchor_to": "bottom_middle",
          "anchor_from": "bottom_middle"
          }
        }
      ]
    }
  }