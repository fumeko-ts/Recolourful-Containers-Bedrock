{
  "namespace": "recolourful_villager",
  "villager_panel@global.root_panel": {
    "size": [ 276, 166 ],
    "layer": 1,
    "anchor_from": "center",
    "anchor_to": "center",
    "$profession_sprite|default": "",
    "$anim_sprite|default": "",
    "controls": [
      {
        "ui@screen_content.image_element": {
          "size": [ 276, 166 ],
          "layer": 1,
          "uv_size": [ 276, 166 ],
          "anchor_to": "center",
          "anchor_from": "center",
          "texture": "assets/sprites/villager"
        }
      },
      {
        "eye_holder@global.empty_panel": {
          "size": [ 160, 54 ],
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "controls": [
            {
              "eye_base@global.empty_panel": {
                "size": [ 12, 5 ],
                "anchor_from": "bottom_left",
                "anchor_to": "bottom_left",
                "controls": [
                  {
                    "eye_animation@screen_content.image_element": {
                      "layer": 3,
                      "uv": "@recolourful_villager.default_eyes_0",
                      "uv_size": [ 12, 5 ],
                      "anchor_from": "center",
                      "animation_reset_name": "screen_animation_reset",
                      "anchor_to": "center",
                      "size": [ 12, 5 ],
                      "texture": "assets/advanced_sprites/animated/villager/default/default"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "label_string_subtraction_bypass@global.empty_panel": {
          "size": [ 170, 166 ],
          "anchor_to": "right_middle",
          "anchor_from": "right_middle",
          "controls": [
            {
              "label_holder@global.empty_stack": {
                "max_size": [ 170, 10 ],
                "min_size": [ 0, 10 ],
                "offset": [ 0, 5 ],
                "orientation": "horizontal",
                "anchor_to": "top_middle",
                "anchor_from": "top_middle",
                "controls": [
                  {
                    "first_label@global.empty_panel": {
                      "size": [ "100%c", 10 ],
                      "controls": [
                        {
                          "label@global.empty_label": {
                            "text": "#final_index",
                            "size": [ "default", 10 ],
                            "color": "$trade_title_text_color",
                            "layer": 2,
                            "bindings": [
                              {"binding_name": "#name_label", "binding_condition": "always"},
                              {
                                "binding_type": "view",
                                "binding_condition": "always",
                                "source_property_name": "(#name_label - 'index_1' - 'index_2' - 'index_3' - 'index_4' - 'index_5')",
                                "target_property_name": "#final_index"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "second_label@global.empty_panel": {
                      "size": [ "100%c", 10 ],
                      "controls": [
                        {
                          "tier_1@recolourful_villager.label_bypass_bindings": {"$villager_mastery": "atemp.trade.level.1", "$villager_tier_index": "index_1"}
                        },
                        {
                          "tier_2@recolourful_villager.label_bypass_bindings": {"$villager_mastery": "atemp.trade.level.2", "$villager_tier_index": "index_2"}
                        },
                        {
                          "tier_3@recolourful_villager.label_bypass_bindings": {"$villager_mastery": "atemp.trade.level.3", "$villager_tier_index": "index_3"}
                        },
                        {
                          "tier_4@recolourful_villager.label_bypass_bindings": {"$villager_mastery": "atemp.trade.level.4", "$villager_tier_index": "index_4"}
                        },
                        {
                          "tier_5@recolourful_villager.label_bypass_bindings": {"$villager_mastery": "atemp.trade.level.5", "$villager_tier_index": "index_5"}
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "1@recolourful_villager.gui_bindings": {
          "$profession_sprite": "armorer",
          "$entity_index": "ui:a",
          "$profession": "armor",
          "$anim_sprite": "assets/advanced_sprites/animated/villager/default/armorer"
