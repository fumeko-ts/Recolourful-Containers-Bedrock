{
  "namespace": "creative_inventory",
  "screen_base@global.root_panel": {
    "size": [195, 192],
    "anchor_to": "center",
    "anchor_from": "center",
    "$is_creative_survival_tab": true,
    "controls": [
        {
        "tab_stack@global.empty_stack": {
          "orientation": "horizontal",
          "size": [195, 32],
          "anchor_to": "top_middle",
          "anchor_from": "top_middle",
          "$is_creative": true,
          "controls": [
              {
              "construction@creative_inventory.construction_tab": {

                }
              },
              {
              "buffer_1": {
                "type": "panel",
                "size": [1, "default"]
                }
              },
              {
              "equipment@creative_inventory.equipment_tab": {

                }
              },
              {
              "buffer_2": {
                "type": "panel",
                "size": [1, "default"]
                }
              },
              {
              "nature@creative_inventory.nature_tab": {

                }
              },
              {
              "buffer_3": {
                "type": "panel",
                "size": [1, "default"]
                }
              },
              {
              "items@creative_inventory.items_tab": {

                }
              },
              {
              "buffer_4": {
                "type": "panel",
                "size": [62, "default"]
                }
              },
              {
              "search_tab@creative_inventory.search_tab": {

                }
              }
            ]
          }
        },
        {
        "inventory@creative_inventory.tab_contents": {
          "$tab_contents_is": "inventory_page",
          "$tab_binding": "inventory_tab_toggle"
          }
        },
        {
        "items@creative_inventory.tab_contents": { "$tab_binding": "construction_tab_toggle" }
        },
        {
        "items@creative_inventory.tab_contents": { "$tab_binding": "equipment_tab_toggle" }
        },
        {
        "items@creative_inventory.tab_contents": { "$tab_binding": "nature_tab_toggle" }
        },
        {
        "items@creative_inventory.tab_contents": { "$tab_binding": "items_tab_toggle" }
        },
        {
        "items@creative_inventory.tab_contents": {
          "$tab_contents_is": "search_page",
          "$tab_binding": "search_tab_toggle"
          }
        },
        {
        "inventory_tab@creative_inventory.inventory_tab": {
          "anchor_to": "bottom_right",
          "anchor_from": "bottom_right"
          }
        },
        {
        "scroll_panel@creative_inventory.scroll_panel": {
          "size": [180, 112],
          "anchor_to": "center",
          "anchor_from": "center",
          "bindings": [
              {
              "binding_type": "view",
              "source_control_name": "inventory_tab_toggle",
              "source_property_name": "(not #toggle_state)",
              "target_property_name": "#visible"
              }
            ]
          }
        }
      ],
    "bindings": [
        {
        "binding_type": "global",
        "binding_name": "#is_creative_mode",
        "binding_name_override": "#visible"
        }
      ]
    },
  "tab_contents@global.empty_panel": {
    "$tab_contents_is": "misc_page",
    "$tab_ui|default": "('creative_inventory.' + $tab_contents_is)",
    "controls": [
        {
        "page@$tab_ui": {
          "bindings": [
              {
              "binding_type": "view",
              "source_control_name": "$tab_binding",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
              }
            ]
          }
        }
      ]
    },
  "scroll_panel@scroll_panel.scroll_panel": {
    "$scroll_track_offset": [81, -0.4],
    "offset": [0.4, 5.4],
    "anchor_to": "center",
    "anchor_from": "center",
    "$scroll_size": [12, 112],
    "size": [181, 112],
    "$scroll_view_port_size": [180, 90.2],
    "$scrolling_pane_size": ["100%", "100%"],
    "$scroll_box_image_control": "creative_inventory.scroller",
    "$scrolling_content": "creative_inventory.scroll_grid_holder"
    },
  "scroller@screen_content.image_element": {
    "tiled": true,
    "nineslice_size": [6, 7, 6, 6],
    "tiled_scale": [1, 1],
    "offset": [2, 0],
    "anchor_to": "center",
    "anchor_from": "center",
    "texture": "assets/minecraft/textures/gui/sprites/container/creative_inventory/scroller.png",
    "size": [12, "100% - 2px"]
    },
  "scroll_grid_holder": {
    "type": "panel",
    "size": [162, "100%cm"],
    "anchor_to": "center",
    "anchor_from": "center",
    "controls": [
        {
        "scroll_buffer@global.empty_panel": { "size": [162, "100%sm"] }
        },
        {
        "scroll_grid": {
          "type": "grid",
          "size": [162, "default"],
          "offset": [0, -0.4],
          "$binding_condition|default": "visible",
          "$grid_item_template|default": "creative_recipe_item.recipe_container_item",
          "collection_name": "$collection_name",
          "$collection_name": "recipe_book",
          "anchor_to": "center",
          "anchor_from": "center",
          "grid_rescaling_type": "horizontal",
          "grid_fill_direction": "vertical",
          "grid_item_template": "$grid_item_template",
          "bindings": [
              {
              "binding_name": "#recipe_book_total_items",
              "binding_name_override": "#maximum_grid_items",
              "binding_condition": "$binding_condition",
              "binding_type": "collection",
              "binding_collection_name": "recipe_book"
              }
            ]
          }
        }
      ]
    },
  "icon_control@screen_content.image_element": {
    "texture": "$icon_for_tab",
    "size": "$tab_icon_size",
    "$icon_for_tab": "",
    "keep_ratio": true,
    "$tab_icon_size": [17, 17],
    "offset": [0, 1],
    "variables": [
        {
        "requires": "($tab_view_binding_name = search_tab_toggle)",
        "$tab_icon_size": [16, 16],
        "$icon_for_tab": "textures/ui/ashur/item/compass_28.png"
        },
        {
        "requires": "($tab_view_binding_name = equipment_tab_toggle)",
        "$tab_icon_size": [16, 16],
        "$icon_for_tab": "textures/ui/ashur/item/iron_sword.png"
        },
        {
        "requires": "($tab_view_binding_name = nature_tab_toggle)",
        "$tab_icon_size": [18, 18],
        "$icon_for_tab": "('textures/ui/ashur/block/' + $nature_tab_texture)"
        },
        {
        "requires": "($tab_view_binding_name = items_tab_toggle)",
        "$tab_icon_size": [16, 16],
        "$icon_for_tab": "textures/ui/ashur/item/redstone"
        },
        {
        "requires": "($tab_view_binding_name = construction_tab_toggle)",
        "$tab_icon_size": [18, 18],
        "$icon_for_tab": "textures/ui/ashur/icon_recipe_construction"
        },
        {
        "requires": "($tab_view_binding_name = inventory_tab_toggle)",
        "$tab_icon_size": [18, 18],
        "$icon_for_tab": "textures/ui/ashur/block/chest"
        }
      ]
    },
  "icon_holder@global.empty_panel": {
    "size": [24, 25],
    "controls": [
        {
        "img@$tab_icon": {

          }
        }
      ]
    },
  "top_tab@toggle.toggle": {
    "type": "panel",
    "size": [26, 32],
    "$toggle_binding_type": "global",
    "$toggle_focus_enabled": true,
    "$needs_hover_text": true,
    "$toggle_name": "navigation_tab",
    "$tab_icon": "creative_inventory.icon_control",
    "$toggle_state_binding_name": "#toggle_state",
    "$tab_content": "creative_inventory.icon_holder"
    },
  "search_tab@creative_inventory.top_tab": {
    "$is_top_right_tab": true,
    "$toggle_hover_text": "Search Items",
    "$tab_view_binding_name": "search_tab_toggle",
    "$toggle_group_forced_index": "$search_index",
    "$toggle_state_binding_name": "#is_left_tab_search"
    },
  "equipment_tab@creative_inventory.top_tab": {
    "$toggle_hover_text": "Equipment Items",
    "$tab_view_binding_name": "equipment_tab_toggle",
    "$toggle_group_forced_index": "$equipment_index",
    "$toggle_state_binding_name": "#is_left_tab_equipment"
    },
  "nature_tab@creative_inventory.top_tab": {
    "$toggle_hover_text": "Nature Items",
    "$tab_view_binding_name": "nature_tab_toggle",
    "$toggle_group_forced_index": "$nature_index",
    "$toggle_state_binding_name": "#is_left_tab_nature"
    },
  "items_tab@creative_inventory.top_tab": {
    "$toggle_hover_text": "Misc Items",
    "$tab_view_binding_name": "items_tab_toggle",
    "$toggle_group_forced_index": "$items_index",
    "$toggle_state_binding_name": "#is_left_tab_items"
    },
  "construction_tab@creative_inventory.top_tab": {
    "$toggle_hover_text": "Construction Items",
    "$is_top_left_tab": true,
    "$tab_view_binding_name": "construction_tab_toggle",
    "$toggle_group_forced_index": "$construction_index",
    "$toggle_state_binding_name": "#is_left_tab_construct"
    },
  "inventory_tab@creative_inventory.top_tab": {
    "$toggle_hover_text": "Inventory",
    "$is_bottom_right_tab": true,
    "$unchecked_offset|default": [0, -2],
    "$toggle_default_state": true,
    "$checked_offset|default": [0, 2],
    "$tab_view_binding_name": "inventory_tab_toggle",
    "$toggle_group_forced_index": "($search_index + 1)"
    },
  "tab_base_template@global.empty_panel": {
    "size": [195, 136],
    "anchor_to": "center",
    "anchor_from": "center"
    },
