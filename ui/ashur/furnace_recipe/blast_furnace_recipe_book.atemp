  "base_page_logic@global.empty_panel": {
    "controls": [
      {
        "base_content@global.empty_panel": {
          "controls": [
            {
              "page_template@global.empty_panel": {
                "clips_children": true,
                "offset": [0.9, -1.6],
                "layer": 4,
                "size": [125, 100],
                "controls": [
                  {"2@blast_furnace_recipe_book.blast_furnace_search": {"layer": 12}},
                  {"3@blast_furnace_recipe_book.blast_furnace_redstone": {"layer": 12}},
                  {"3@blast_furnace_recipe_book.blast_furnace_ore": {"layer": 12}}
                ]
              }
            },
            {
              "holder@global.empty_stack": {
                "orientation": "vertical",
                "anchor_to": "top_middle",
                "anchor_from": "top_middle",
                "size": [111, 30],
                "offset": [8, 0],
                "controls": [
                  { "buffer_1@global.empty_panel": { "size": [111, 11] } },
                  {
                    "holder@global.empty_stack": {
                      "orientation": "horizontal",
                      "size": [111, 18],
                      "controls": [
                        {
                          "text_edit_panel@global.empty_panel": {
                            "size": [81, 18],
                            "controls": [
                              {
                                "Misc@smelting_common.title_template": {
                                }
                              }
                            ]
                          }
                        },
                        { "buffer_2@global.empty_panel": { "size": ["fill", "fill"] } },
                        {
                          "filter": {
                            "type": "toggle",
                            "size": [26, 16],
                            "unchecked_control": "default",
                            "unchecked_hover_control": "default_hover",
                            "controls": [
                              { "default@smelting_common.furnace_filter_on": {} },
                              {
                                "default_hover@smelting_common.furnace_filter_on_hover": {
                                  "controls": [
                                    {
                                      "hover_text@common.hover_text": {
                                        "layer": 400,
                                        "property_bag": {
                                          "#hover_text": "currently the recipe book\nand its contents are\nvisual only"
                                        }
                                      }
                                    }
                                  ]
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }

