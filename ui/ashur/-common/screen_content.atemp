      "controls": [
         {
            "close@global.close_button": {}
         },
         {
            "all_misc@$container": {}
         }
      ]
   },
   "close_button_image@screen_content.image_element": {
      "keep_ratio": true,
      "texture": "$close_button_texture"
   },
   "highlight_slot@screen_content.image_element": {
      "texture": "textures/ui/ashur/white_slot",
      "size": "$white_highlight_slot_size",
      "offset": "$white_highlight_slot_offset",
      "$white_highlight_slot_offset|default": [0, 0],
      "$white_highlight_slot_size|default": [16, 16],
      "alpha": 0.45
   },
   "label": {
      "type": "label",
      "offset": "$text_offset",
      "$text_offset": [0, 0],
      "anchor_from": "top_left",
      "anchor_to": "top_left",
      "text": "$container_title",
      "size": ["90%", "default"],
      "color": "$title_text_color",
      "layer": 20
   },
