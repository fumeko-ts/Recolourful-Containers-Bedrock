  "cell_image@global.empty_image": {
    "size": ["default", "default"],
    "bindings": [
        {
        "binding_name": "(#container_item_background_texture = $texture_path)",
        "binding_name_override": "#container_item_background_texture",
        "binding_type": "collection",
        "binding_collection_name": "$item_collection_name"
        }
      ]
    },
  "recipe_container_cell_images": {
    "type": "panel",
    "size": [18, 18],
    "controls": [
        {
        "item_group_bg@creative_recipe_item.slot": { "$texture_path": "('textures/ui/recipe_book_group_bg' or 'textures/ui/recipe_book_group_expanded' or 'textures/ui/recipe_book_group_collapsed')" }
        },
        {
        "default_item_bg@creative_recipe_item.slot": { "$texture_path": "('textures/ui/recipe_book_item_bg')" }
        },
        {
        "red_bg@creative_recipe_item.slot": { "$texture_path": "('textures/ui/recipe_book_red_button')" }
        },
        {
        "selected@creative_recipe_item.slot": {
          "$texture_path": "('textures/ui/recipe_book_touch_cell_selected')",
          "controls": [
              {
              "recipe_book_touch_cell_selected@creative_recipe_item.slot": {
                "property_bag": {
                  "#collection_name": "crafting_output_items",
                  "#collection_index": 0
                  },
                "bindings": [
                    {
                    "binding_name": "(not (#container_item_background = 3))",
                    "binding_name_override": "#visible",
                    "binding_type": "collection",
                    "binding_collection_name": "crafting_output_items",
                    "binding_condition": "always"
                    }
                  ]
                }
              },
              {
              "recipe_book_red_button@creative_recipe_item.slot": {
                "property_bag": {
                  "#collection_name": "crafting_output_items",
                  "#collection_index": 0
                  },
                "bindings": [
                    {
                    "binding_name": "(#container_item_background = 3)",
                    "binding_name_override": "#visible",
                    "binding_type": "collection",
                    "binding_collection_name": "crafting_output_items",
                    "binding_condition": "always"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
  "slot@creative_recipe_item.cell_image": {
