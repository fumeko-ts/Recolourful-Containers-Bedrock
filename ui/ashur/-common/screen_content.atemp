      "texture": "assets/minecraft/textures/gui/sprites/container/enchanting_table/enchantment_slot_highlighted",
      "controls": [
         {
            "tooltip@global.hover_text": {
               "bindings": [
                  {
                     "binding_name": "#hover_text",
                     "binding_collection_name": "#enchant_buttons",
                     "binding_type": "collection"
                  },
                  {
                     "binding_type": "view",
                     "binding_condition": "always",
                     "source_property_name": "('§r' + #hover_text - '§o')",
                     "target_property_name": "#hover_text"
                  }
               ]
            }
         }
      ]
   },
   "redstone_activated_bindings@screen_content.image_element": {
      "size": "$image_size",
      "$image_size": [16, 16],
      "layer": 12,
      "texture": "$arrow_texture"
   },
   "text_field@screen_content.image_element": {
      "nineslice_size": 1,
      "texture": "assets/minecraft/textures/gui/sprites/widget/text_field",
      "size": ["100%", "100%"]
   },
   "trade_toggle_unchecked@trade_global.button_state_common": {
      "$image_texture|default": "assets/minecraft/textures/gui/sprites/widget/button",
      "layer": 4
   },
   "trade_toggle_checked@trade_global.button_state_common": {
      "$image_texture|default": "assets/minecraft/textures/gui/sprites/widget/button_highlighted",
      "layer": 4
