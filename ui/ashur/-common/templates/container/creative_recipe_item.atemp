  "recipe_container_item": {
    "type": "input_panel",
    "size": [18, 18],
    "layer": 1,
    "$button_ref": "creative_recipe_item.no_coalesce_container_slot_button",
    "$background_images": "creative_recipe_item.recipe_container_cell_images",
    "$item_collection_name": "recipe_book",
    "$overlay_images|default": "crafting.container_overlay_images",
    "$cell_overlay_ref|default": "common.cell_overlay",
    "$item_renderer|default": "common.item_renderer",
    "$item_renderer_panel_size|default": [18, 18],
    "$item_renderer_size|default": [16, 16],
    "$item_renderer_offset|default": [0, 0],
    "$cell_image_size|default": [18, 18],
    "$background_image_control_name|default": "bg",
    "focus_enabled": true,
    "focus_wrap_enabled": false,
    "focus_magnet_enabled": true,
    "tts_ignore_count": true,
    "$button_ref|default": "creative_recipe_item.survival_container_slot_button",
    "$overlay_image_control_name|default": "overlay",
    "controls": [
        {
        "item_cell": {
          "type": "panel",
          "size": [18, 18],
          "layer": 0,
          "controls": [
              {
              "$background_image_control_name@$background_images": { "layer": 1 }
              },
              {
              "item": {
                "type": "panel",
                "size": "$item_renderer_panel_size",
                "layer": 0,
                "controls": [
                    {
                    "$item_renderer@$item_renderer": {
                      "size": "$item_renderer_size",
                      "offset": "$item_renderer_offset",
                      "anchor_to": "center",
                      "anchor_from": "center",
                      "layer": 7
                      }
                    }
                  ]
                }
              },
              {
              "$overlay_image_control_name@$overlay_images": { "layer": 9 }
              }
            ]
          }
        },
        {
        "item_cell_overlay_ref@$cell_overlay_ref": { "layer": 3 }
        },
        {
        "item_button_ref@$button_ref": {
          "tts_ignore_count": true,
          "tts_skip_message": true,
          "tts_inherit_siblings": true,
          "layer": 5
          }
        }
      ]
    },
