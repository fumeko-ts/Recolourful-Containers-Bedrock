  "search_page@creative_inventory.tab_base_template": {
    "controls": [
        {
        "search_holder@global.empty_stack": {
          "orientation": "vertical",
          "size": [116, 16],
          "anchor_to": "top_right",
          "anchor_from": "top_right",
          "layer": 20,
          "controls": [
              {
              "filler_1@global.empty_panel": { "size": ["default", 4] }
              },
              {
              "search_extra_padding@global.empty_stack": {
                "orientation": "horizontal",
                "size": [116, "default"],
                "controls": [
                    {
                    "text_edit_panel@global.empty_panel": {
                      "size": [90, 12],
                      "controls": [
                          {
                          "text_edit_control@text_edit_box.text_edit_box": {
                            "size": [90, 12],
                            "property_bag": { "#property_field": "#item_name" },
                            "max_length": 300000,
                            "anchor_from": "center",
                            "anchor_to": "center",
                            "$text_box_text_color": "white",
                            "$text_edit_box_label_offset": [1, 0.4],
                            "$text_clear_button_enabled": false,
                            "$place_holder_text": "",
                            "$text_background_locked": "global.empty_image",
                            "$text_background_default": "global.empty_image",
                            "$text_background_hover": "global.empty_image",
                            "$text_edit_binding_name": "#text_box_item_name",
                            "$text_edit_box_label_size": ["default", 10]
                            }
                          }
                        ]
                      }
                    },
                    {
                    "filler_2@global.empty_panel": { "size": [26, "default"] }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
        "search_tab_base@screen_content.creative_inventory_search": {
